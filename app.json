    },
    "android": {
      "package": "com.uxd.gakko",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-1264500349297774~8614563059"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
