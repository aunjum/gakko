{
  "expo": {
    "plugins": [
      [
        "expo-ads-admob",
        {
          "userTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ]
    ],
    "name": "gakko",
    "slug": "gakko",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
