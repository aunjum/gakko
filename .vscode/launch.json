    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Android",
        "program": "${workspaceFolder}\\gakko\\App.js",
        "type": "reactnative",
        "request": "launch",
        "platform": "android",
        "sourceMaps": true,
        "outDir": "${workspaceRoot}/.vscode/.react"
      },
      {
        "name": "Debug iOS",
        "program": "${workspaceFolder}\\gakko\\App.js",
        "type": "reactnative",
        "request": "launch",
        "platform": "ios",
        "sourceMaps": true,
        "outDir": "${workspaceRoot}/.vscode/.react"
      },
      {
        "name": "Attach to packager",
        "program": "${workspaceFolder}\\gakko\\App.js",
        "type": "reactnative",
        "request": "attach",
        "sourceMaps": true,
        "outDir": "${workspaceRoot}/.vscode/.react"
      },
      {
        "name": "Chrome Attach to packager",
        "program": "${workspaceFolder}\\gakko\\App.js",
        "type": "chrome",
        "request": "attach",
        "port": 9222,
        "sourceMaps": true,
        "outDir": "${workspaceRoot}/.vscode/.react"
      },
      {
        "name": "Debug in Exponent",
        "program": "${workspaceFolder}\\gakko\\App.js",
        "type": "reactnative",
        "request": "launch",
        "platform": "exponent",
        "sourceMaps": true,
        "outDir": "${workspaceRoot}/.vscode/.react"
      }
    ]
  }